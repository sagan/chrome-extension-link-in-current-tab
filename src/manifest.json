{
    "name": "Link in Current Tab",
    "description": "Force all links to be opened in current tab instead of the new one",
    "version": "1.0.2",
    "author": "Jacques De SAGAN",
    "manifest_version": 2,
    "permissions": [],
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "run_at": "document_start",
            "all_frames": true
        }
    ]
}